{% extends "layout.html.twig" %}

{% block title %}Home{% endblock %}

{% block content %}
{% for article in articles %}
<article>
    <div><a class="articleTitle" href="{{ path('article', { 'id': article.id }) }}">{{ article.title }}</a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;{{ article.dateAjout|date("d-m-Y H:i:s") }}</div></br>
    <p>{{ article.content }}</p>
    
</article>
{% endfor %}
{% endblock %}
